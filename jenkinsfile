pipeline {
    agent any
    stages {
        stage ('checkout') {
            stepe {
                sh '''
                ls
                echo "hi amar"
                '''
            }
        }
        stage ('test') {
            steps {
                sh '''
                #!/bin/bash
                echo "hi"
                '''
            }
        }  
        stage('build') {
            steps {
                sh '''
                mvn clean install
                '''
            }
        }
    }
}