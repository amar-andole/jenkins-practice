pipeline {
    agent any
    stages ('TEST MULTIPLE')
        parallel {
            stage ('test1') {
                steps {
                sh '''
                sleep 5
                '''
                }
            }
            stage ('test2') {
                steps {
                    sh '''
                    echo "amar"
                    sleep 5
                    '''
                }
            }
            stage('test3') {
                steps {
                    sh '''
                    echo "hi amar"
                    sleep 5
                    '''
                }
            }
        }
    }