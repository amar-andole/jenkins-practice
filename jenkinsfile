pipeline {
    agnet any
    stages {
        stage ('checkout') {
            steps{
                checkout([$class: 'GitSCM', branches: [[name: '*/main']],
                userRemoteConfigs: [[url:'https://github.com/jenkins-docs/simple-java-maven-app.git' ]]])
            }
        }
        stage ('build') {
            steps {
                sh '''
                ls -lrt
                mvn clean install
                '''
            }
        }
    }
}